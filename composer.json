{
  "name": "fajuu/flarum-icons",
  "description": "Finally, you can change the forum icons to the one you like!",
  "type": "flarum-extension",
  "keywords": ["fajuu", "fontawesomepro","icons"],
  "homepage": "https://github.com/Vell61/flarum-icons",
  "license": "MIT",
  "authors": [
    {
      "name": "Fajuu",
      "email": "Fajuu@mykill.pl",
      "homepage": "https://mykill.pl"
    }
  ],
  "support": {
    "issues": "https://github.com/Vell61/flarum-icons/issues",
    "source": "https://github.com/Vell61/flarum-icons"
  },
  "require": {
    "flarum/core": ">=1.3.1"
  },
  "autoload": {
    "psr-4": {"Fajuu\\Icons\\": "src/"}
  },
  "extra": {
    "flarum-extension": {
      "title": "Fajuu Icons",
      "icon": {
        "name": "fas fa-shapes",
        "backgroundColor": "#1c141f",
        "color": "#8e24aa"
      }
    },
    "flagrow": {
      "discuss": "https://discuss.flarum.org/d/21401"
    }
  }
}
